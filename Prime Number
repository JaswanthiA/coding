import java.util.*;
class Prime
{
public static void main(String args[])
{
int count=0;
Scanner x=new Scanner(System.in);
System.out.println("Enter n value");
int n=x.nextInt();
for(int i=1;i<=n;i++)
{
if(n%i==0)
{
count++;
}
}
if(count==2)
System.out.println("Is a prime number");
else
System.out.println("it is not a prime number");
}
}
